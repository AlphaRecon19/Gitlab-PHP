{
    "name": "alpharecon19/gitlab-php",
    "authors": [
        {
            "name": "Chris Hilsdon",
            "email": "chris@koolserve.uk"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.1",
        "symfony/yaml" : "2 - 3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "squizlabs/php_codesniffer": "2.*",
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Gitlab\\": ["src/"]
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Gitlab\\Tests\\" : "tests/Gitlab"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    }
}
